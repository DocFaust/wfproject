<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<VBox xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="de.docfaust.mp3synchronizer.javafx.control.SetFilterController">
	<Label text="%setfilter.extensions"/>
	<TextField fx:id="fldNewExtension" promptText="%setfilter.prmtnewextension"/>
	<ListView fx:id="listExtensions"/>
	<HBox>
	<Button fx:id="btnAdd" text="%setfilter.btnadd" onAction="#handleAdd"/>
	<Button fx:id="btnDelete" text="%setfilter.btndelete" onAction="#handleDelete"/>
	<Button fx:id="btnOk" text="%setfilter.btnok" onAction="#handleOk"/>
	<Button fx:id="btnCancel" text="%setfilter.btncancel" onAction="#handleCancel"/>
	</HBox>
</VBox>

